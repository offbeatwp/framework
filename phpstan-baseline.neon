parameters:
	ignoreErrors:
		-
			message: '#^Method OffbeatWP\\Blocks\\AbstractBlockCallback\:\:getInnerBlocks\(\) never returns array\{\} so it can be removed from the return type\.$#'
			identifier: return.unusedType
			count: 1
			path: src/Blocks/AbstractBlockCallback.php

		-
			message: '#^Call to deprecated method registerWidget\(\) of class OffbeatWP\\Components\\ComponentRepository\.$#'
			identifier: method.deprecated
			count: 1
			path: src/Components/ComponentRepository.php

		-
			message: '#^Unreachable statement \- code above always terminates\.$#'
			identifier: deadCode.unreachable
			count: 1
			path: src/Console/AbstractCommand.php

		-
			message: '#^Method OffbeatWP\\Content\\Post\\WpPostsIterator\:\:current\(\) never returns false so it can be removed from the return type\.$#'
			identifier: return.unusedType
			count: 1
			path: src/Content/Post/WpPostsIterator.php

		-
			message: '#^Offset 1 on array\{string, non\-falsy\-string, ''''\|''_num'', non\-empty\-string\} in isset\(\) always exists and is not nullable\.$#'
			identifier: isset.offset
			count: 1
			path: src/Content/Post/WpQueryBuilder.php

		-
			message: '#^Offset 1 on array\{string, non\-falsy\-string, ''''\|''_num'', non\-empty\-string\} in isset\(\) always exists and is not nullable\.$#'
			identifier: isset.offset
			count: 1
			path: src/Content/Taxonomy/TermQueryBuilder.php

		-
			message: '#^Method OffbeatWP\\Content\\User\\UserQueryBuilder\:\:limit\(\) has no return type specified\.$#'
			identifier: missingType.return
			count: 1
			path: src/Content/User/UserQueryBuilder.php

		-
			message: '#^Method OffbeatWP\\Content\\User\\UserQueryBuilder\:\:whereIdNotIn\(\) has no return type specified\.$#'
			identifier: missingType.return
			count: 1
			path: src/Content/User/UserQueryBuilder.php

		-
			message: '#^Offset 1 on array\{string, non\-falsy\-string, ''''\|''_num'', non\-empty\-string\} in isset\(\) always exists and is not nullable\.$#'
			identifier: isset.offset
			count: 1
			path: src/Content/User/UserQueryBuilder.php

		-
			message: '#^PHPDoc tag @return has invalid value \(\$this\<TValue\>\)\: Unexpected token "\<", expected TOKEN_HORIZONTAL_WS at offset 17 on line 1$#'
			identifier: phpDoc.parseError
			count: 1
			path: src/Content/User/UserQueryBuilder.php

		-
			message: '#^PHPDoc tag @return has invalid value \(\$this\<TValue\>\)\: Unexpected token "\<", expected TOKEN_HORIZONTAL_WS at offset 53 on line 3$#'
			identifier: phpDoc.parseError
			count: 1
			path: src/Content/User/UserQueryBuilder.php

		-
			message: '#^Access to undefined constant static\(OffbeatWP\\Form\\Fields\\AbstractField\)\:\:FIELD_TYPE\.$#'
			identifier: classConstant.notFound
			count: 1
			path: src/Form/Fields/AbstractField.php

		-
			message: '#^Unsafe usage of new static\(\)\.$#'
			identifier: new.static
			count: 1
			path: src/Form/Fields/AbstractField.php

		-
			message: '#^Parameter \#1 \$options of method OffbeatWP\\Form\\Fields\\Select\:\:addOptions\(\) expects array\<int\|string, bool\|float\|int\|string\|null\>, array\<int, \$this\(OffbeatWP\\Form\\Fields\\NavMenu\)\|string\> given\.$#'
			identifier: argument.type
			count: 1
			path: src/Form/Fields/NavMenu.php

		-
			message: '#^Template type T of function container\(\) is not referenced in a parameter\.$#'
			identifier: method.templateTypeNotInParameter
			count: 1
			path: src/Foundation/helpers.php

		-
			message: '#^Template type T of function offbeat\(\) is not referenced in a parameter\.$#'
			identifier: method.templateTypeNotInParameter
			count: 1
			path: src/Foundation/helpers.php

		-
			message: '#^Possibly impure call to function array_filter\(\) in pure method OffbeatWP\\Helpers\\ArrayHelper\:\:toIntArray\(\)\.$#'
			identifier: possiblyImpure.functionCall
			count: 1
			path: src/Helpers/ArrayHelper.php

		-
			message: '#^Possibly impure call to function array_map\(\) in pure method OffbeatWP\\Helpers\\ArrayHelper\:\:toIntArray\(\)\.$#'
			identifier: possiblyImpure.functionCall
			count: 1
			path: src/Helpers/ArrayHelper.php

		-
			message: '#^Call to function is_array\(\) with array\<mixed\> will always evaluate to true\.$#'
