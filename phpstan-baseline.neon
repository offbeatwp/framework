		-
			message: '#^Method OffbeatWP\\Views\\Wordpress\:\:shortcode\(\) never returns null so it can be removed from the return type\.$#'
			identifier: return.unusedType
			count: 1
			path: src/Views/Wordpress.php

		-
			message: '#^Method OffbeatWP\\Views\\Wordpress\:\:title\(\) never returns null so it can be removed from the return type\.$#'
			identifier: return.unusedType
			count: 1
			path: src/Views/Wordpress.php

		-
			message: '#^Parameter \$args of method OffbeatWP\\Views\\Wordpress\:\:navMenu\(\) has invalid type OffbeatWP\\Views\\WP_Term\.$#'
			identifier: class.notFound
			count: 1
			path: src/Views/Wordpress.php
